{
    "name": "Node.js",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "forwardPorts": [
        8000,
        8001
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "GitHub.copilot",
                "DavidAnson.vscode-markdownlint",
                "esbenp.prettier-vscode",
                "shardulm94.trailing-spaces",
                "dbaeumer.vscode-eslint",
                "formulahendry.auto-close-tag",
                "unifiedjs.vscode-mdx",
                "redhat.vscode-yaml",
                "ms-azuretools.vscode-docker"
            ]
        }
    }
}